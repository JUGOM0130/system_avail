<h1><%= @title %></h1>
<ul>
  <li><a href="/usermasta/create">登録</a></li>
  <li><a href="/usermasta/delete">全削除</a></li>
</ul>
<%= form_with model:[@user_data], url:"/kengen/update" do |f| %>
  <div class="my-2">
    <%= f.label :user_cd, "ユーザコード",for:"user_cd", class:"form-label" %>
    <%= f.text_field :user_cd,value: @user_data[:user_cd], class: "form-control", readonly:true %>
  </div>
  <div class="my-2">
    <%= f.label :k_level, "権限レベル",for:"k_level", class:"form-label" %>
    <%= f.text_field :k_level,value: @user_data[:k_level], class: "form-control" %>
  </div>
  <div class="my-2">
    <%= f.submit "登録", class:"btn btn-success"%>
  </div>

<% end %>