<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">コード</th>
                <th scope="col">名前</th>
            </tr>
        </thead>
        <% @Users.each do |user| %>
        <tbody>
            <tr>
                <th scope="row"><%= "#{user.id}" %></th>
                <td><%= "#{user.cd}" %></td>
                <td><%= "#{user.name}" %></td>
            </tr>
        </tbody>
        <%end%>
    </table>
</div>