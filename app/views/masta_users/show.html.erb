
    <h1 class="mt-3 mb-5"><%= @title %></h1>
    <ul>
        <li><a href="/usermasta/create">登録</a></li>
        <li><a href="/usermasta/delete">全削除</a></li>
    </ul>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">コード</th>
                <th scope="col">名前</th>
                <th scope="col">氏</th>
                <th scope="col">名</th>
            </tr>
        </thead>
        <% @data.each do |o| %>
        <tbody>
            <tr>
                <th scope="row"><%= "#{o.id}" %></th>
                <td><a href="/usermasta/update/<%= "#{o.cd}"%>"><%= "#{o.cd}"%></a></td>
                <td><%= "#{o.name}" %></td>
                <td><%= "#{o.first_name}" %></td>
                <td><%= "#{o.last_name}" %></td>
            </tr>
        </tbody>
        <%end%>
    </table>

