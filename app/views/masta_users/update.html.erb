
    <h1 class="mt-3 mb-5"><%= @title %></h1>

    <%= form_with model: @user, url:"up", method: :post do |f| %>
    <% @user.each do |o|%>
    <div class="row">
        <div class="col">
            <div class="mb-3">
                <% #for: は無くても自動生成してくれるみたい %>
                <%= f.label :user_cd, "ユーザコード",for:"user_cd", class:"form-label" %>
                <%= f.text_field :user_cd, placeholder:"0000", class:"form-control", value: o.cd, readonly: "true"%>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="mb-3">
                    <% #for: は無くても自動生成してくれるみたい %>
                    <%= f.label :first_name, "苗字", for:"first_name", class:"form-label" %>
                    <%= f.text_field :first_name, placeholder:"氏", class:"form-control",value:o.first_name%>
                </div>
            </div>
            <div class="col">
                <div class="mb-3">
                    <%= f.label :last_name, "名前", for:"last_name", class:"form-label" %>
                    <%= f.text_field :last_name, placeholder:"名", class:"form-control",value:o.last_name%>
                </div>
            </div>
        </div>
    </div>
    <%end%>
    <%= f.submit "登録", class:"btn btn-info"%>
    <% end %>
    <p></p>
    <p></p>

<script>

</script>
