<h1 class="mt-3 mb-5"><%= @title %></h1>

<%= form_with model: @model do |f| %>
<div class="row">
    <div class="col">
        <div class="mb-3">
            <%= f.label :tcd, "取引先コード",for:"tcd", class:"form-label" %>
            <%= f.text_field :tcd, class:"form-control", readonly:true%>
        </div>
    </div>
</div>
<div class="row">
    <div class="col">
        <div class="mb-3">
            <%= f.label :tname, "取引先名（漢字）",for:"tname", class:"form-label" %>
            <%= f.text_field :tname, class:"form-control"%>
        </div>
    </div>
    <div class="col">
        <div class="mb-3">
            <%= f.label :tnname, "取引先名（ひらがな）",for:"tnname", class:"form-label" %>
            <%= f.text_field :tnname, class:"form-control"%>
        </div>
    </div>
</div>
<div class="row">
    <div class="col">
        <div class="mb-3">
            <%= f.label :tyubin_no, "郵便番号",for:"tyubin_no", class:"form-label" %>
            <%= f.text_field :tyubin_no, class:"form-control"%>
        </div>
    </div>
    <div class="col">
        <div class="mb-3">
            <%= f.label :tjusho1, "住所（県）",for:"tjusho1", class:"form-label" %>
            <%= f.text_field :tjusho1, class:"form-control"%>
        </div>
    </div>
    <div class="col">
        <div class="mb-3">
            <%= f.label :tjusho2, "住所（市）",for:"tjusho2", class:"form-label" %>
            <%= f.text_field :tjusho2, class:"form-control"%>
        </div>
    </div>
</div>
<div class="row">
    <div class="col">
        <div class="mb-3">
            <%= f.label :tjusho3, "住所（丁目）",for:"tjusho3", class:"form-label" %>
            <%= f.text_field :tjusho3, class:"form-control"%>
        </div>
    </div>
    <div class="col">
        <div class="mb-3">
            <%= f.label :tjusho4, "住所（区）",for:"tjusho4", class:"form-label" %>
            <%= f.text_field :tjusho4, class:"form-control"%>
        </div>
    </div>
    <div class="col">
        <div class="mb-3">
            <%= f.label :tjusho5, "住所（その他）",for:"tjusho5", class:"form-label" %>
            <%= f.text_field :tjusho5, class:"form-control"%>
        </div>
    </div>
</div>

<%= f.submit "登録", class:"btn btn-info"%>

<% end %>
